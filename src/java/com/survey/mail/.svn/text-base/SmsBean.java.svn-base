package com.survey.mail;

/**
 *
 * @author Ritesh
 */
public class SmsBean {
    private String smsUserName = "";
    private String smsUserPassword = "";
    private String smsSenderId = "";
    private String smsHost ="";
    private String recipientNo = "";
    private String msgText = "";
    private int state = 0;

    public String getMsgText() {
        return msgText;
    }

    public void setMsgText(String msgText) {
        this.msgText = msgText;
    }

    public String getRecipientNo() {
        return recipientNo;
    }

    public void setRecipientNo(String recipientNo) {
        this.recipientNo = recipientNo;
    }

    public String getSmsHost() {
        return smsHost;
    }

    public void setSmsHost(String smsHost) {
        this.smsHost = smsHost;
    }

    public String getSmsSenderId() {
        return smsSenderId;
    }

    public void setSmsSenderId(String smsSenderId) {
        this.smsSenderId = smsSenderId;
    }

    public String getSmsUserName() {
        return smsUserName;
    }

    public void setSmsUserName(String smsUserName) {
        this.smsUserName = smsUserName;
    }

    public String getSmsUserPassword() {
        return smsUserPassword;
    }

    public void setSmsUserPassword(String smsUserPassword) {
        this.smsUserPassword = smsUserPassword;
    }

    public int getState() {
        return state;
    }

    public void setState(int state) {
        this.state = state;
    }


  







}
